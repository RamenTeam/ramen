		"graphql-request": "^3.4.0",
		"graphql-subscriptions": "^1.1.0",
		"graphql-tag": "^2.11.0",
		"graphql-tools": "^7.0.2",
		"graphql-yoga": "^1.18.3",
		"graphql-yup-middleware": "0.0.1",
		"ioredis": "^4.19.4",
		"isomorphic-fetch": "^3.0.0",
		"jest": "^26.6.3",
		"merge-graphql-schemas": "^1.7.8",
		"mock-session": "0.0.5",
		"nodemon": "^2.0.7",
		"pg": "^8.5.1",
		"reflect-metadata": "^0.1.13",
		"request-promise": "^4.2.6",
		"sofa-api": "^0.2.3",
		"subscriptions-transport-ws": "^0.9.18",
		"swagger-ui-express": "^4.1.6",
		"ts-jest": "^26.5.3",
		"ts-node": "^3.3.0",
		"type-graphql": "^1.1.1",
		"typedi": "^0.8.0",
		"typeorm": "^0.2.25",
		"typeorm-naming-strategies": "^2.0.0",
		"typeorm-typedi-extensions": "^0.4.0",
		"typescript": "^4.1.3",
		"uuid": "^7.0.0",
		"yup": "^0.32.8"
	},
	"scripts": {
		"start": "ts-node src/index.ts",
		"dev": "concurrently \" npx cross-env redis-server \" \" SET NODE_ENV=development& ts-node-dev --respawn src/index.ts \" \" npm run gen-gql \" ",
		"dev:test": "concurrently \" npx cross-env redis-server \" \" SET NODE_ENV=test& ts-node-dev --respawn src/index.ts \" \" npm run gen-gql \"",
		"test": "jest --watch --runInBand",
		"ts:build": "tsc",
		"ts:watch": "tsc -w",
		"serve": "SET NODE_ENV=production& nodemon dist/src/index.js",
		"prod:serve": "node dist/src/index.js --experimental-modules",
		"gen-gql": "gqlg --schemaFilePath ./src/schema.graphql --destDirPath ./src/modules/graphql --depthLimit 5",
		"gen-env": "npx gen-env-types .env -o src/types/env.d.ts -e .",
		"gen-rest-api-docs": "ts-node ./src/scripts/genOpenAPI.ts"
	},
	"resolutions": {
		"graphql": "^15.0.0"
	}
}
