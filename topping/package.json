{
	"name": "graphql-apollo-ts",
	"version": "0.0.1",
	"main": "dist/index",
	"typings": "dist/index",
	"type": "module",
	"description": "Awesome project developed with TypeORM.",
	"devDependencies": {
		"@types/bcrypt": "^3.0.0",
		"@types/connect-redis": "0.0.16",
		"@types/express-session": "^1.17.3",
		"@types/faker": "^5.1.7",
		"@types/graphql": "^14.5.0",
		"@types/ioredis": "^4.19.1",
		"@types/node": "^8.10.66",
		"ts-node-dev": "^1.1.1"
	},
	"dependencies": {
		"@babel/register": "^7.13.8",
		"@types/jest": "^26.0.20",
		"@types/uuid": "^8.3.0",
		"bcrypt": "^5.0.0",
		"body-parser": "^1.19.0",
		"class-validator": "^0.13.1",
		"connect-redis": "^5.0.0",
		"core-js": "3.0.0-beta.11",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"express-session": "^1.17.1",
		"faker": "^5.4.0",
		"fetch-cookie": "^0.11.0",
		"graphql": "^15.3.0",
		"graphql-import": "^1.0.2",
